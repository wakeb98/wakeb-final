{
    "type": "APL",
    "version": "1.8",
    "theme": "dark",
    "import": [
        {
            "name": "alexa-layouts",
            "version": "1.5.0"
        }
    ],
"mainTemplate": {
    "parameters": [
        "payload"
    ],
    "items": [
        {
            "type": "Pager",
            "id": "fisrtpager",
            "width": "100%",
            "height": "100%",
            "items": [
            ],
            "navigation": "none",
            "onMount": [{
  "type": "OpenURL",
  "source": "http://demo.wakeb.tech/swcc-text-to-voice/public",
  "onFail": {
    "type": "SetValue",
    "componentId": "errorText",
    "property": "text",
    "value": "Unable to open Amazon.com (${event.source.value})"
  }
            }]
        }
    ]
}
}